  "description": "Wrapper for a logging package.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "pino": "^7.6.5"
  },
  "devDependencies": {
    "@tsconfig/node10": "^1.0.7",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
